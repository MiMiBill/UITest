<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity4">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:shapeAppearance="@style/RoundedStyle"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        app:layout_constraintTop_toBottomOf="@id/img1"
        app:layout_constraintStart_toStartOf="parent"
        app:shapeAppearance="@style/circleStyle"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        android:padding="3dp"
        app:layout_constraintTop_toBottomOf="@id/img2"
        app:layout_constraintStart_toStartOf="parent"
        app:shapeAppearance="@style/circleStyle"
        app:strokeColor="@color/black"
        app:strokeWidth="3dp"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        android:padding="3dp"
        app:layout_constraintTop_toBottomOf="@id/img3"
        app:layout_constraintStart_toStartOf="parent"
        app:shapeAppearance="@style/cutStyle"
        app:strokeColor="@color/black"
        app:strokeWidth="3dp"
        />


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        android:padding="3dp"
        app:layout_constraintTop_toBottomOf="@id/img4"
        app:layout_constraintStart_toStartOf="parent"
        app:shapeAppearance="@style/RhombusStyle"
        app:strokeColor="@color/black"
        app:strokeWidth="3dp"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        android:padding="3dp"
        app:layout_constraintTop_toBottomOf="@id/img5"
        app:layout_constraintStart_toStartOf="parent"
        app:shapeAppearance="@style/LeafStyle"
        app:strokeColor="@color/black"
        app:strokeWidth="3dp"
        />


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        android:padding="3dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/img1"
        app:shapeAppearance="@style/SemicircleStyle"
        app:strokeWidth="3dp"
        app:strokeColor="@color/black"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img8"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        android:padding="3dp"
        app:layout_constraintTop_toBottomOf="@id/img7"
        app:layout_constraintStart_toEndOf="@id/img1"
        app:shapeAppearance="@style/HexagonStyle"
        app:strokeWidth="3dp"
        app:strokeColor="@color/black"
        />

    <TextView
        android:id="@+id/img9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="30dp"
        android:layout_marginStart="10dp"
        android:scaleType="centerCrop"
        android:text="尚山峰"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/img8"
        app:layout_constraintStart_toEndOf="@id/img1"
        />

    <TextView
        android:id="@+id/img10"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="30dp"
        android:scaleType="centerCrop"
        android:text="尚山峰"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/img9"
        app:layout_constraintStart_toEndOf="@id/img1"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/img11"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/meinv"
        android:padding="3dp"
        app:layout_constraintTop_toBottomOf="@id/img10"
        app:layout_constraintStart_toEndOf="@id/img1"
        app:shapeAppearance="@style/topRoundStyle"
        app:strokeColor="@color/black"
        app:strokeWidth="3dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>